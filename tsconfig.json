{
	"compilerOptions": {
    "outDir": "./dist/",
    "rootDir": "./src/",
		"sourceMap": true,
		"noImplicitAny": true,
		"module": "commonjs",
		"target": "es6",
		"jsx": "react",
		"baseUrl": "./",
		"paths": {
			"@/*": ["src/*"],
			"@components/*": ["src/components/*"],
			"@services/*": ["src/services/*"],
			"@redux/*": ["src/redux/*"],
			"@actions/*": ["src/redux/actions/*"],
			"@sagas/*": ["src/redux/sagas/*"]
		}
	}
}
